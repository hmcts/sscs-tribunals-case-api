{
  "request": {
    "method": "GET",
    "urlPathPattern": "/letters/[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}",
    "queryParameters": {
      "include-additional-info": {
        "equalTo": "false"
      },
      "check-duplicate": {
        "equalTo": "true"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "letter_id": "{{randomValue type='UUID'}}"
    },
    "transformers": ["response-template"]
  }
}
